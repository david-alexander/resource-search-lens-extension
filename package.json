{
	"name": "resource-search-lens-extension",
	"version": "0.0.1",
	"description": "Lens extension providing a VS Code style Ctrl+P search for Kubernetes resources.",
	"publisher": "resource-search-lens-extension",
	"license": "MIT",
	"main": "dist/main.js",
	"renderer": "dist/renderer.js",
	"engines": {
		"node": ">=12.0",
		"lens": ">=5"
	},
	"contributes": {
		"crds": [],
		"cloudProviders": [],
		"kubernetesDistros": []
	},
	"keywords": [
		"lens",
		"extension",
		"k8slens",
		"kubernetes"
	],
	"files": [
		"package.json",
		"dist/main.js",
		"dist/renderer.js"
	],
	"scripts": {
		"start": "cross-env NODE_ENV=development webpack --watch",
		"build": "npm run clean && cross-env NODE_ENV=production webpack",
		"clean": "rimraf ./dist"
	},
	"dependencies": {
		"@k8slens/extensions": "^5.0.0-beta.7",
		"@types/node": "^12.12.9",
		"@types/react": "^17.0.0",
		"cross-env": "^7.0.3",
		"electron": "^11.0.3",
		"mobx": "^6.3.2",
		"mobx-react": "^7.2.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"rimraf": "^3.0.2",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.11"
	}
}
